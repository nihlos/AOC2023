file = open('input.txt', 'r')
file = file.read().split('\n')

firstNumber = ''
firstNumberIndex = 99999
secondNumber = ''
secondNumberIndex = -1
numberList = [['one', 'two', 'three', 'four', 'five', 'six', 'seven', 'eight', 'nine'],
              ['1', '2', '3', '4', '5', '6', '7', '8', '9']]
sum = 0

for i in range(len(file)):
    for j in range(len(numberList)):
        for k in numberList[j]:
            if k in file[i]:
                if file[i].index(k) < firstNumberIndex:
                    firstNumberIndex = file[i].index(k)
                    firstNumber = k

        for k in reversed(numberList[j]):
            if k in file[i]:
                if file[i].index(k) > secondNumberIndex:
                    secondNumberIndex = file[i].index(k)
                    secondNumber = k

    if firstNumberIndex == secondNumberIndex:
        secondNumber = ''

    if firstNumber != '' and firstNumber in numberList[0]:
        firstNumber = firstNumber.replace(firstNumber, str(numberList[0].index(firstNumber) + 1))
    if secondNumber != '' and secondNumber in numberList[0]:
        secondNumber = secondNumber.replace(secondNumber, str(numberList[0].index(secondNumber) + 1))

    print(firstNumber + ", " + secondNumber)

    if firstNumber != '' and secondNumber != '':
        sum += int(firstNumber + secondNumber)
    elif firstNumber != '':
        sum += int(firstNumber)
    elif secondNumber != '':
        sum += int(secondNumber)

    firstNumber = ''
    firstNumberIndex = 99999
    secondNumber = ''
    secondNumberIndex = -1

print(sum)
